// Place your key bindings in this file to override the defaults
[
    //
    // View
    //
    {
        "key": "shift+ctrl+alt+o",
        "command": "outline.focus",
        "when": "!isMac && focusedView != 'outline'"
    },
    {
        "key": "shift+ctrl+alt+o",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!isMac && focusedView == 'outline'"
    },
    {
        "key": "shift+alt+cmd+o",
        "command": "outline.focus",
        "when": "isMac && focusedView != 'outline'"
    },
    {
        "key": "shift+alt+cmd+o",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "isMac && focusedView == 'outline'"
    },
    {
        "key": "ctrl+alt+h",
        "command": "timeline.focus",
        "when": "!isMac && focusedView != 'timeline'"
    },
    {
        "key": "ctrl+alt+h",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!isMac && focusedView == 'timeline'"
    },
    {
        "key": "alt+cmd+h",
        "command": "timeline.focus",
        "when": "isMac && focusedView != 'timeline'"
    },
    {
        "key": "alt+cmd+h",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "isMac && focusedView == 'timeline'"
    },
    {
        "key": "ctrl+alt+g",
        "command": "gitlens.views.fileHistory.focus",
        "when": "!isMac && focusedView != 'gitlens.views.fileHistory' && focusedView != 'terminal'"
    },
    {
        "key": "ctrl+alt+g",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!isMac && focusedView == 'gitlens.views.fileHistory'"
    },
    {
        "key": "alt+cmd+g",
        "command": "gitlens.views.fileHistory.focus",
        "when": "isMac && focusedView != 'gitlens.views.fileHistory'"
    },
    {
        "key": "alt+cmd+g",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "isMac && focusedView == 'gitlens.views.fileHistory'"
    },
    {
        "key": "ctrl+alt+l",
        "command": "gitlens.views.lineHistory.focus",
        "when": "!isMac && focusedView != 'gitlens.views.lineHistory'"
    },
    {
        "key": "ctrl+alt+l",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!isMac && focusedView == 'gitlens.views.lineHistory'"
    },
    {
        "key": "alt+cmd+l",
        "command": "gitlens.views.lineHistory.focus",
        "when": "isMac && focusedView != 'gitlens.views.lineHistory'"
    },
    {
        "key": "alt+cmd+l",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "isMac && focusedView == 'gitlens.views.lineHistory'"
    },
    //
    // Editor Split
    //
    {
        "key": "ctrl+alt+\\",
        "command": "workbench.action.splitEditorDown",
        "when": "!isMac"
    },
    {
        "key": "alt+cmd+\\",
        "command": "workbench.action.splitEditorDown",
        "when": "isMac"
    },
    //
    // Copy path
    //
    {
        "key": "ctrl+alt+c",
        "command": "copyFilePath",
        "when": "!isMac && !fileMatchOrFolderMatchWithResourceFocus"
    },
    {
        "key": "ctrl+alt+c",
        "command": "search.action.copyPath",
        "when": "!isMac && fileMatchOrFolderMatchWithResourceFocus"
    },
    {
        "key": "ctrl+shift+alt+c",
        "command": "copyRelativeFilePath",
        "when": "!isMac && !fileMatchOrFolderMatchWithResourceFocus"
    },
    //
    // Reveal in OS
    //
    {
        "key": "shift+alt+r",
        "command": "revealFileInOS",
        "when": "resourceScheme == 'file' || resourceScheme == 'vscode-userdata'"
    },
    //
    // Join Lines
    //
    {
        "key": "ctrl+shift+j",
        "command": "editor.action.joinLines",
        "when": "!isMac && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+j",
        "command": "editor.action.joinLines",
        "when": "isMac && editorTextFocus && !editorReadonly"
    },
    //
    // Format
    //
    {
        "key": "shift+alt+f",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "shift+alt+f",
        "command": "editor.action.formatDocument.none",
        "when": "editorTextFocus && !editorHasDocumentFormattingProvider && !editorReadonly"
    },
    {
        "key": "ctrl+k ctrl+f",
        "command": "editor.action.formatChanges",
        "when": "!isMac && !editorHasSelection"
    },
    {
        "key": "cmd+k cmd+f",
        "command": "editor.action.formatChanges",
        "when": "isMac && !editorHasSelection"
    },
    //
    // suggest
    //
    {
        "key": "shift+escape",
        "command": "editor.action.inlineSuggest.hide",
        "when": "inlineSuggestionVisible"
    },
    //
    // indent & outdent
    //
    {
        "key": "cmd+[",
        "command": "-editor.action.outdentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+]",
        "command": "-editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+[",
        "command": "editor.action.outdentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+]",
        "command": "editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    //
    // workbench navigation
    //
    {
        "key": "ctrl+shift+/",
        "command": "workbench.action.toggleActivityBarVisibility",
        "when": "!isMac && !terminalFocus"
    },
    {
        "key": "cmd+shift+/",
        "command": "workbench.action.toggleActivityBarVisibility",
        "when": "isMac"
    },
    {
        "key": "ctrl+alt+0",
        "command": "workbench.action.focusAuxiliaryBar",
        "when": "!isMac"
    },
    {
        "key": "alt+cmd+0",
        "command": "workbench.action.focusAuxiliaryBar",
        "when": "isMac"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.focusPanel",
        "when": "!panelFocus"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "panelFocus && editorAreaVisible"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.toggleMaximizedPanel",
        "when": "panelFocus && !editorAreaVisible"
    },
    //
    // editor navigation
    //
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor",
        "when": "!inQuickOpen"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor",
        "when": "!inQuickOpen"
    },
    // for use in integrated terminal on chromeOS
    {
        "key": "cmd+p",
        "command": "workbench.action.quickOpen",
        "when": "!isMac"
    },
    {
        "key": "alt+q",
        "command": "workbench.action.showAllEditorsByMostRecentlyUsed",
        "when": "!inQuickOpen"
    },
    {
        "key": "alt+q",
        "command": "workbench.action.quickOpenNavigateNextInFilePicker",
