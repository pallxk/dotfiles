#!name=Aliyun Shortcuts
#!desc=Redirect short URLs to websites
#!category=Aliyun

[MITM]
hostname = %APPEND% ack, cs, acr, cr, cts, dbs, dts, dtsnew, dysms, ebs, eci, ecs, hbr, kafka, kafkanext, kms, kvstore, mpaas, oss, polardb, quotas, rds, rdsnext, slb, vpc, vpc2vpc, vpn, vpngw, waf, yaochi

[Script]
aliyun-shortcuts.js = type=http-request,pattern=^https?://(ack|cs|acr|cr|cts|dbs|dts|dtsnew|dysms|ebs|eci|ecs|hbr|kafka|kafkanext|kms|kvstore|mpaas|oss|polardb|quotas|rds|rdsnext|slb|vpc|vpc2vpc|vpn|vpngw|waf|yaochi)/,script-path=modules/aliyun-shortcuts.js
