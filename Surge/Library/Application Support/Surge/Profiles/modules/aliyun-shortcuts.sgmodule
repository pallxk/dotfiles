#!name=Aliyun Shortcuts
#!desc=Redirect short URLs to websites
#!category=Aliyun

[MITM]
hostname = %APPEND% ack, acl, alb, clb, cdn, cs, acr, cr, cts, dbs, dms, dts, dtsnew, dysms, ebs, eci, ecs, hbr, kafka, kafkanext, kms, kvstore, mpaas, nlb, oss, polardb, quotas, rds, rdsnext, sg, slb, vpc, vpc2vpc, vpn, vpngw, vtb, waf, yaochi

[Script]
aliyun-shortcuts.js = type=http-request,pattern=^https?://(ack|acl|alb|clb|cdn|cs|acr|cr|cts|dbs|dms|dts|dtsnew|dysms|ebs|eci|ecs|hbr|kafka|kafkanext|kms|kvstore|mpaas|nlb|oss|polardb|quotas|rds|rdsnext|sg|slb|vpc|vpc2vpc|vpn|vpngw|vtb|waf|yaochi)/,script-path=modules/aliyun-shortcuts.js
