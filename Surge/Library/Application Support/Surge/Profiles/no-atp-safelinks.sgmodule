[MITM]
hostname = *.safelinks.protection.outlook.com, statics.teams.cdn.office.net

[Script]
no-atp-safelinks.js = type=http-request,pattern=^https://.+.safelinks.protection.outlook.com,script-path=no-atp-safelinks.js
no-atp-safelinks.js = type=http-request,pattern=^https://statics.teams.cdn.office.net/evergreen-assets/safelinks/1/atp-safelinks.html,script-path=no-atp-safelinks.js
